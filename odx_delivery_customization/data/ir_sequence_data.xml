<odoo>
    <record id="sequence_delivery_picking" model="ir.sequence">
       <field name="name">Delivery picking</field>
       <field name="code">delivery.picking</field>
       <field name="active">TRUE</field>
       <field name="prefix">DP</field>
       <field name="padding">5</field>
       <field name="number_next">1</field>
       <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
      <record id="sequence_work_details" model="ir.sequence">
       <field name="name">Work Details</field>
       <field name="code">work.details</field>
       <field name="active">TRUE</field>
       <field name="prefix">WK</field>
       <field name="padding">5</field>
       <field name="number_next">1</field>
       <field name="number_increment">1</field>
          <field name="company_id" eval="False"/>
    </record>
     <record id="sequence_settlement_details" model="ir.sequence">
       <field name="name">Settlement Details</field>
       <field name="code">settlement.details</field>
       <field name="active">TRUE</field>
       <field name="prefix">SD</field>
       <field name="padding">5</field>
       <field name="number_next">1</field>
       <field name="number_increment">1</field>
         <field name="company_id" eval="False"/>
    </record>

</odoo>
