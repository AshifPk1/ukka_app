<odoo>
    <record id="vendor_report_wizrad_view" model="ir.ui.view">
        <field name="name">vendor.report.wiz</field>
        <field name="model">vendor.report.wiz</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="date_from"/>
                    </group>
                    <group>
                        <field name="date_to"/>
                    </group>
                </group>
                <group>
                    <group>
                        <!--                        <field name="vendor_id"/>-->
                        <field name="vendor_id" domain="[('is_delivery', '=', False),('supplier_rank','>=',1)]"/>
                    </group>
                    <group>
                        <field name="branch_id"/>
                    </group>
                </group>

                <footer>
                    <button string="Get Report" type="object" class="oe_highlight" name="get_data"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="vendor_report_wiz_action" model="ir.actions.act_window">
        <field name="name">Vendor Report</field>
        <field name="res_model">vendor.report.wiz</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <menuitem id="vendor_report_menu" name="Vendor Report" parent="sale.menu_sale_report"
              action="vendor_report_wiz_action" sequence="2"/>
</odoo>